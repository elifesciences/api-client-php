{
    "name": "elife/api-client",
    "description": "eLife Sciences API client",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "eLife\\ApiClient\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\eLife\\ApiClient\\": "./spec",
            "test\\eLife\\ApiClient\\": "./test"
        }
    },
    "require": {
        "php": "^7.0",
        "beberlei/assert": "^2.2",
        "crell/api-problem": "^2.0",
        "guzzlehttp/promises": "^1.0",
        "guzzlehttp/psr7": "^1.0",
        "ocramius/package-versions": "^1.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "giorgiosironi/eris": "^0.8",
        "guzzlehttp/guzzle": "^6.0",
        "mtdowling/jmespath.php": "^2.0",
        "phpspec/phpspec": "^2.4",
        "phpspec/prophecy": "^1.10",
        "phpunit/phpunit": "^5.2",
        "psr/log": "^1.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "suggest": {
        "guzzlehttp/guzzle": "^6.0, to use the Guzzle6HttpClient adapter",
        "mtdowling/jmespath.php": "^2.0, to use the Result::search() method",
        "psr/log": "^1.0, to use the WarningCheckingHttpClient adapter",
        "monolog/monolog": "^1.0, to use the HttpMessageProcessor log processor"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
